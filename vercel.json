{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/(images|logo\\.png)(.*)",
      "dest": "/$1$2"
    },
    {
      "src": "/(.*\\.php)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/api/index.php"
    }
  ]
}
