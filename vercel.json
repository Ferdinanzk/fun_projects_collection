{
    "version": 2,
    "builds": [
        {
            "src": "public/index.php",
            "use": "@vercel/php"
        },
        {
            "src": "public/api/*.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/public/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "functions": {
        "public/api/*.php": {
            "runtime": "vercel-php@0.7.4"
        }
    }
}

